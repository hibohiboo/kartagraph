{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "bundle": "cdk synth > cdk.out/template.yml",
    "pipeline-deploy": "cdk deploy --require-approval never --all",
    "deploy": "cdk deploy --profile produser"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.0.0",
    "@types/aws-lambda": "^8.10.85",
    "@types/node": "16.11.11",
    "aws-cdk": "2.0.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.1",
    "npm-check-updates": "^12.0.2",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.2"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.134.0",
    "@aws-cdk/aws-cloudfront": "^1.134.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.134.0",
    "@aws-cdk/aws-iam": "^1.134.0",
    "@aws-cdk/aws-route53": "^1.134.0",
    "@aws-cdk/aws-route53-targets": "^1.134.0",
    "@aws-cdk/aws-s3": "^1.134.0",
    "@aws-cdk/aws-s3-deployment": "^1.134.0",
    "@aws-cdk/core": "1.134.0",
    "source-map-support": "^0.5.21"
  }
}
