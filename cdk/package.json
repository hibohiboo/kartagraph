{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "bundle": "cdk synth > cdk.out/template.yml",
    "pipeline-deploy": "cdk deploy --require-approval never --all",
    "predeploy": "node build.js",
    "deploy": "cdk deploy aws-cartagraph-stack aws-cartagraph-deploy-stack --profile produser"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.68.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "20.3.1",
    "aws-cdk": "2.84.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.4",
    "npm-check-updates": "^16.10.12",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.84.0",
    "constructs": "^10.2.52",
    "source-map-support": "^0.5.21"
  }
}