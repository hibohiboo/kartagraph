{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "bundle": "cdk synth > cdk.out/template.yml",
    "pipeline-deploy": "cdk deploy --require-approval never --all",
    "predeploy": "node build.js",
    "deploy": "cdk deploy aws-cartagraph-stack aws-cartagraph-deploy-stack --profile produser"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.63.2",
    "@types/aws-lambda": "^8.10.110",
    "@types/node": "18.13.0",
    "aws-cdk": "2.63.2",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.6",
    "npm-check-updates": "^16.6.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.63.2",
    "constructs": "^10.1.244",
    "source-map-support": "^0.5.21"
  }
}