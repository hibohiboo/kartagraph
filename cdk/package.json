{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "bundle": "cdk synth > cdk.out/template.yml",
    "pipeline-deploy": "cdk deploy --require-approval never --all",
    "deploy": "cdk deploy aws-cartagraph-stack aws-cartagraph-deploy-stack --profile produser"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.27.0",
    "@types/aws-lambda": "^8.10.98",
    "@types/node": "17.0.39",
    "aws-cdk": "2.27.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.42",
    "npm-check-updates": "^13.1.1",
    "ts-node": "^10.8.1",
    "typescript": "~4.7.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.27.0",
    "constructs": "^10.1.27",
    "source-map-support": "^0.5.21"
  }
}