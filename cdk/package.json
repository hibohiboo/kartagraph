{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "bundle": "cdk synth > cdk.out/template.yml",
    "pipeline-deploy": "cdk deploy --require-approval never --all",
    "deploy": "cdk deploy --profile produser"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.12.0",
    "@types/aws-lambda": "^8.10.92",
    "@types/node": "17.0.18",
    "aws-cdk": "2.12.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.22",
    "npm-check-updates": "^12.3.0",
    "ts-node": "^10.5.0",
    "typescript": "~4.5.5"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.12.0",
    "constructs": "^10.0.64",
    "source-map-support": "^0.5.21"
  }
}
