{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "bundle": "cdk synth > cdk.out/template.yml",
    "pipeline-deploy": "cdk deploy --require-approval never --all",
    "deploy": "cdk deploy --profile produser"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.16.0",
    "@types/aws-lambda": "^8.10.93",
    "@types/node": "17.0.21",
    "aws-cdk": "2.16.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.25",
    "npm-check-updates": "^12.5.3",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.16.0",
    "constructs": "^10.0.85",
    "source-map-support": "^0.5.21"
  }
}
